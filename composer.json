{
  "name": "bitexpert/magerun2-list-api-endpoints",
  "description": "Plugin for netz98 Magerun2 to list all API endpoints",
  "keywords": [
    "magerun"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "homepage": "https://github.com/philippsander/magerun2-list-api-endpoints",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Sander",
      "email": "p.sander@bitexpert.de",
      "homepage": "http://www.bitexpert.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "n98/magerun2": "2.1.*",
    "magento/module-webapi": "100.2.1",
    "magento/framework": "101.0.*",
    "phpunit/phpunit": "^6.5",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "autoload": {
    "psr-0": {
      "Bitexpert": "src/",
      "N98": "vendor/n98/magerun/src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "N98": "vendor/n98/magerun/tests"
    }
  },
  "scripts": {
    "check": [
      "@cs-check",
      "@test"
    ],
    "cs-check": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "cs-fix": "vendor/bin/phpcbf",
    "test": "vendor/bin/phpunit"
  }
}
